# WireGuard Maximum Obfuscation Setup - Environment Variables
# Скопируйте этот файл в .env и отредактируйте под свои нужды
# cp .env.example .env

# =================
# ОСНОВНЫЕ НАСТРОЙКИ
# =================

# Внешний IP адрес или домен вашего сервера
SERVER_ENDPOINT=your-server.com

# Пароль для веб-интерфейса wg-easy
WG_EASY_PASSWORD=your-secure-password-here

# Порт веб-интерфейса
WEB_PORT=51821

# Порт для WireGuard
WG_PORT=51820

# Подсеть для VPN клиентов
WG_SUBNET=10.8.0.x

# Разрешенные IP для клиентов (обычно 0.0.0.0/0 для полного VPN)
WG_ALLOWED_IPS=0.0.0.0/0

# Keep-alive для мобильных клиентов (секунды)
WG_KEEPALIVE=25

# MTU для оптимизации
WG_MTU=1280

# ======================
# ПАРАМЕТРЫ ОБФУСКАЦИИ
# ======================

# AmneziaWG обфускация - количество мусорных пакетов
JC=5

# Минимальный размер мусорного пакета
JMIN=100

# Максимальный размер мусорного пакета
JMAX=1000

# Magic header значения (можно сгенерировать случайно)
S1=86
S2=92

# Magic hash значения (можно сгенерировать случайно)
H1=1234567890
H2=9876543210
H3=1122334455
H4=5544332211

# ===============
# DNS НАСТРОЙКИ
# ===============

# Основные DNS серверы (AdGuard публичные по умолчанию)
ADGUARD_DNS_IP1=94.140.14.14
ADGUARD_DNS_IP2=94.140.15.15

# DNS-over-HTTPS (для персонального AdGuard используйте свой URL)
ADGUARD_DNS_HTTPS=https://dns.adguard-dns.com/dns-query

# DNS-over-TLS (для персонального AdGuard используйте свой URL)
ADGUARD_DNS_TLS=tls://dns.adguard-dns.com

# Персональные AdGuard DNS (если есть)
# ADGUARD_DNS_HTTPS=https://d.adguard-dns.com/dns-query/your-device-id
# ADGUARD_DNS_TLS=tls://your-device-id.d.adguard-dns.com

# ===================
# ДОПОЛНИТЕЛЬНЫЕ ПОРТЫ
# ===================

# Порт для веб-статистики
STATS_PORT=8080

# Включить Cloak обфускацию (true/false)
CLOAK_ENABLED=false

# Cloak порты
CLOAK_HTTPS_PORT=8443
CLOAK_HTTP_PORT=8080

# =================
# ГЕНЕРАЦИЯ КОНФИГОВ
# =================

# Настройки для мобильных конфигов
MOBILE_KEEPALIVE=25
MOBILE_MTU=1280

# Настройки для роутеров
ROUTER_KEEPALIVE=0
ROUTER_MTU=1420

# Настройки для десктопов
DESKTOP_KEEPALIVE=0
DESKTOP_MTU=1420

# ==================
# БЕЗОПАСНОСТЬ
# ==================

# Автоматическое обновление ключей (дни)
AUTO_KEY_ROTATION_DAYS=30

# Максимальное количество клиентов
MAX_CLIENTS=50

# Логирование подключений (true/false)
ENABLE_LOGGING=true

# Уровень логирования (debug, info, warn, error)
LOG_LEVEL=info

# =================
# ДОПОЛНИТЕЛЬНО
# =================

# Язык интерфейса
LANG=ru

# Включить статистику трафика
UI_TRAFFIC_STATS=true

# Тип графиков (1=линия, 2=область)
UI_CHART_TYPE=2

# Одноразовые ссылки для конфигов
WG_ENABLE_ONE_TIME_LINKS=true

# Домен для SSL сертификатов (если используется Nginx proxy)
DOMAIN=your-domain.com

# Email для Let's Encrypt
LETSENCRYPT_EMAIL=your-email@example.com
