# RouteRus Configuration
# Copy to .env and fill in your values.
# SECURITY: Never commit the .env file â€” it contains secrets.

# ===== Domain Configuration =====
PANEL_DOMAIN="panel.duckdns.org"
REALITY_DOMAIN="reality.duckdns.org"

# ===== Routing Features =====
ENABLE_ADBLOCK="y"
ENABLE_RU_ROUTING="y"
ENABLE_QUIC_BLOCK="y"

# ===== Advanced Settings =====
UPDATE_GEOFILES="y"

# Panel credentials (leave empty for auto-generation)
PANEL_USERNAME=""
PANEL_PASSWORD=""

# ===== Telegram Bot (Optional) =====
# Get a token from @BotFather, never share it publicly
TELEGRAM_BOT_TOKEN=""
TELEGRAM_ADMIN_ID=""

# ===== Custom DNS (Optional) =====
CUSTOM_DNS1="1.1.1.1"
CUSTOM_DNS2="8.8.8.8"

# ===== Cloudflare (Optional, not recommended for REALITY) =====
USE_CLOUDFLARE="n"
CLOUDFLARE_EMAIL=""
CLOUDFLARE_API_KEY=""

# ===== SSH Port (Optional) =====
CUSTOM_SSH_PORT="22"
